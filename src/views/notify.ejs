<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
        integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <script src="main.js"></script>
    <link href="/css/style.css" rel="stylesheet" type="text/css">

    <script src="https://code.jquery.com/jquery-3.5.0.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6/jquery.min.js" type="text/javascript"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js" type="text/javascript"></script>
    <link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="Stylesheet"
        type="text/css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet" type="text/css">
    <title>DETAILS NOTIFY</title>
    <link rel="icon" href="/img/icons/node-js.svg" color="#5bbad5">

</head>

<body>
    <!-- Notification Detail -->
    <div class="home-header-contaier">

        <div class="left_area">
            <div class="row">
                <a href="/" style="text-decoration: none;">
                    <h3>THÔNG <span>BÁO</span></h3>
                </a>
            </div>

            <div class="home-header-nameProfile">
                <span class="nameProfile"></span>
            </div>

            <div class="right_area">
                <div class="home-header-imgProfile">
                    <i class="fas fa-user"></i>
                </div>
                <ul>
                    <a href="/">
                        <li><i class="fa fa-user" aria-hidden="true"></i>Trang cá nhân</li>
                    </a>
                    <% if (auth==='1' ){ %>
                        <a href="/register">
                            <li><i class="fa fa-inbox" aria-hidden="true"></i>Tạo phòng ban</li>
                        </a>
                        <% }%>
                            <a href="/logout">
                                <li class="logout-profile"><i class="fa fa-sign-out-alt" aria-hidden="true"></i>Logout
                                </li>
                            </a>
                </ul>
            </div>
        </div>
        <div class="home-nav-container">


            <div class="home-nav-sidebar">
                <div class="home-nav-sidebarList">

                    <div class="home-nav-imgProfile">

                    </div>
                    <ul class="home-nav-sidebarListFeat">

                        <li>
                            <a class="/"><i class="fa fa-home"></i><span title="Trang chủ">Trang chủ</span></a>
                        </li>
                        <li>
                            <a class="home-nav-categoryMessage first"><i class="fas fa-list"></i><span class="first"
                                    title="Phân loại thông báo">Đơn vị</span><span
                                    class="fas fa-chevron-left showList first"></span></a>

                            <nav>
                                <ul class="home-nav-listMessage">

                                    <li>
                                        <a href="/phongban" class="donvi"><i class="fas fa-users"></i><span
                                                title="Đơn vị">Khoa/Phòng ban</span></a>

                                    </li>
                                </ul>
                            </nav>
                        </li>
                        <li>
                            <div class="logout-profile">

                                <a href="/logout"><i class="fas fa-sign-out-alt"></i><span>Logout</span></a>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="home-nav-contentStatus">
                <div class="home-nav-Status">
                    <div class="row row-status">

                        <div class="col-sm-8">
                            <div class="postStatus">

                                <div class="row postStatus-row">

                                    <div class="timeline">

                                        <div class="inner-main-body p-2 p-sm-3 notification-content">
                                            <div class="card">
                                                <div class="card-body">
                                                    <div class="media notification-item">
                                                        <div class="media-body ml-3">
                                                            <a class="text-secondary">
                                                                <%= thongbaos.user.name %>
                                                            </a>
                                                            <small class="text-time ml-2">
                                                                <%= thongbaos.createdAt %>
                                                            </small>
                                                            <h5 class="mt-1">
                                                                <%= thongbaos.title %>
                                                            </h5>
                                                            <div class="text-notify mt-3">
                                                                <p>
                                                                    <%= thongbaos.text %>

                                                                </p>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>

                                        </div>


                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class='datepicker'>
                                <div class="datepicker-header"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</body>

</html>